    "react-intersection-observer": "^9.13.1",
    "resize-observer-polyfill": "^1.5.1",
    "shallowequal": "^1.1.0",
    "stylelint": "^14.1.0",
    "stylelint-config-standard": "^24.0.0",
    "tsx": "^4.6.2",
    "typescript": "^4.0.0",
    "vitest": "^1.5.0",
    "yorkie": "^2.0.0"
  },
  "files": [
    "es",
    "less",
    "compiled/wechat/src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ant-design/ant-design-mini.git",
